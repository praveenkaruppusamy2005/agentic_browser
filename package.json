{
  "name": "electron-basic-template",
  "version": "0.1.0",
  "private": true,
  "description": "Basic Electron app template",
  "main": "src/main.js",
  "type": "module",
  "scripts": {
    "start": "electron .",
    "start:dev": "cross-env ELECTRON_START_URL=http://localhost:3000 electron .",
    "dev": "concurrently \"npm:renderer\" \"npm:start:dev\"",
    "renderer": "cd renderer && vite"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^39.2.4",
    "vite": "^5.0.10"
  },
  "dependencies": {
    "lottie-react": "^2.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-modal": "^3.16.3"
  }
}
