{
  "name": "electron-basic-template",
  "version": "0.1.0",
  "private": true,
  "description": "Basic Electron app template",
  "main": "src/main.js",
  "type": "module",
  "scripts": {
    "start": "electron .",
    "start:dev": "cross-env ELECTRON_START_URL=http://localhost:3000 electron .",
    "dev": "concurrently \"npm:renderer\" \"npm:start:dev\"",
    "renderer": "cd renderer && vite"
